<!-- Our web map and content will go here -->
<div id="map"></div>
<script>
var mymap = L.map('map', {
    center: [35.5946, -82.5540]
    zoom: 11,
    maxZoom: 18,
    minZoom: 11,
    detectRetina: true
});
L.titleLayer('http://{s}.basemaps.cartocdn.com/light_all/{z}/{x}/{y}.png').addTo(mymap);
</script>
